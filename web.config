<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpPlatformHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="C:\inetpub\wwwroot\api\cep\.venv\Scripts\python.exe"
                  arguments="app.py"
                  stdoutLogEnabled="true"
                  stdoutLogFile="C:\inetpub\wwwroot\api\cep\logs\cep.log"
                  startupTimeLimit="20" />
  </system.webServer>
</configuration>
